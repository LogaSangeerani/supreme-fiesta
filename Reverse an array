package array;

public class ReverseAnArray {

	public static void main(String[] args) {
		// Reverse an array
		
		int a[]={7,3,5,9,10,6};
		
		for(int i=0;i<a.length/2;i++)
		{
			int temp=a[i];
			a[i]=a[a.length-1-i];
			a[a.length-1-i]=temp;
		}
		
		for(int i=0;i<a.length;i++)
		{
			System.out.print(a[i]+" ");
		}
	}

}
