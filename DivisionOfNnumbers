package javaBasics;

import java.util.Scanner;

public class DivisionOfNnumbers {

	public static void main(String[] args) {
		
		
		// Division of n numbers from 1
		
				int n,i=2;
				double div=1.0;
				
				Scanner sc= new Scanner(System.in);
				System.out.println("Enter number of value to be added");
				n=sc.nextInt();
				
				while(i<=n)
				{
					div=div/i;
					i++;
				}
				System.out.println(div);
			}

	}


