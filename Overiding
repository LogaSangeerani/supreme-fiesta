package javaBasics;

class Parent
{
	public void work()
	{
		System.out.println("Parent is under retirement from work");
	}
}

public class Child extends Parent {
	
	public void work()
	{
		System.out.println("Child has a job");
		System.out.println("He is doing it well");
	}

	public static void main(String[] args) {
		
		// Overiding
		
		Child c1 = new Child();
		c1.work();
	}

}
