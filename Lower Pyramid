package patternpgm;

public class LowerPyramid {

	public static void main(String[] args) {
		
		int n=5;
		for(int i=1;i<=n;i++)
		{
			
			for(int j=n;j>=i;j--)
			{
				System.out.print("* ");
			}
			System.out.println();
			
			for(int sp=1;sp<=i;sp++)
			{
				System.out.print(" ");
			}
		}


	}

}
