package javaset2;

public class CountNumOftimesAnElement {

	public static void main(String[] args) {
		// Count how many times an element is present
		
		int a[]={3,7,2,8,7,7};
		int count=0;
		
		for(int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length;j++)
			{
			if(a[i]==a[j])
				count++;
			}
				
		}
		
		System.out.println("The number of times 8 is present in the given array : "+count);

	}

}

output:
The number of times 8 is present in the given array : 3
