package javaBasics;

public class SumOfDigitsModel5 {

	public static void main(String[] args) {
		
		// (value%100), (value/100)

				int box = 0;
				int value = 1234;
				
				while(value > 0)
				{
					int res = value % 100;
					value = value / 100;
					box = box + res;
				}
				
				System.out.println(box);


	}

}
