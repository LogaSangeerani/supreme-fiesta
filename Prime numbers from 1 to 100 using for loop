package javaBasics;

public class PrimeNumberTillHundred {

	public static void main(String[] args) {
		
		// Prime numbers from 1 to 100
		
		int count, num;
		
		for(num=2;num<=100;num++)
		{
			count=0;
			for(int i=2;i<=num/2;i++)
			{
				if(num%i==0)
				{
					count++;
					break;
				}
			}
			if(count==0)
			{
				System.out.println(num + " ");
			}
	
		}
		
	}
	
}
