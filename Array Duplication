package array;

public class ArrayDuplication {

	public static void main(String[] args) {
		// remove array duplication
		
		int a[]={8,5,2,4,3,8};
		
		for(int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length;j++)
			{
				if(a[i]==a[j])
				a[j]=0;
			}
		}
		for(int i=0;i<a.length;i++)
		{
			if(a[i]!=0)
			System.out.print(a[i]+" ");
		}
		

	}

}
