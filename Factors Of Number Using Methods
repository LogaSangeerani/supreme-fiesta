package javaBasics;

import java.util.Scanner;

public class FactorsOfNumberUsingMethods {

	private static Scanner sc;

	public static void main(String[] args) {
		int n;
		sc = new Scanner(System.in);
        System.out.println("Please enter any num to find factors");
        n=sc.nextInt();
        
        FactorsOfNumber(n);
        }
	
	public static void FactorsOfNumber(int n)
	{
		int i;
		for(i=1;i<=n;i++)
		{
			if(n % i == 0)
			{
				System.out.print(" "+i);
			}
		}
	}

}
