package javaBasics;

public class SumOfDigitsModel6 {

	public static void main(String[] args) {
		
		// (value>1)

				int box = 0;
				int value = 1234;
				
				while(value > 1)
				{
					int res = value % 10;
					value = value / 10;
					box = box + res;
				}
				
				System.out.println(box);

	}

}
